            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Cumque Pyrēnēī regressus ad intima saltūs,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Mīlite cum lassō"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=" callēs trānscenderet artōs"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Īnsidiās eius summō sub vertice montis"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Tendēre Wascōnēs ausī, nova praelia temptant."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Dēnique postrēmōs populī rēgālis adortī,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Missilibus prīmō sternunt ex collibus altīs,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Et Francōs, quamvīs"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" armīs animīsque priōrēs,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Impār fēcit et angustus locus īnferiōrēs."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Rēx iam praecessit, tardumque remānserat agmen,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Cūra vehendārum"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" quod rērum praepediēbat."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Fit pavor hinc exercitibus, subitōque tumultū"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Turbantur, victrīx latrōnum turba nefandā"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Ingentem rapuit praedam, plūrēsque necāvit."/>
                <trail PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="136.001" YPOS="25396.2802547771" OwnPage="59" ItemID="762729381" PTYPE="4" WIDTH="522" HEIGHT="150.811745222938" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 150.812 L0 150.812 L0 0 Z" copath="M0 0 L522 0 L522 150.812 L0 150.812 L0 0 Z" gXpos="136.001" gYpos="25396.2802547771" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="(When) with his weary soldier(s) he (Charlemagne) crossed the narrow path, the Basques dared to stage an ambush under the highest peak of the mountain, trying to wage new battle. "/>
                <ITEXT CH=" "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="mīlites"/>
                <ITEXT CH=" is best translated as a collective singular: witih weary soldiers."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="And the Franks who were below, although they were better (than the Basques) in arms and spirit, the narrow place made unequal (to the Basques)."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Already the king had proceeded (through the mountain pass) and the slow army had remained, which the care of carrying the things had delayed."/>
                <ITEXT CH=" The Basques attacked the baggage train at the rear of Charlemagne's army."/>
                <trail PARENT="footnotes"/>
            </StoryText>
        </PAGEOBJECT>
