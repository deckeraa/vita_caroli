        <PAGEOBJECT XPOS="136.001" YPOS="888.005" OwnPage="1" ItemID="568036021" PTYPE="4" WIDTH="522" HEIGHT="235.561878980892" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 235.562 L0 235.562 L0 0 Z" copath="M0 0 L522 0 L522 235.562 L0 235.562 L0 0 Z" gXpos="136.001" gYpos="888.005" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Salvtor mund, postquam d Virgine nsc"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Digntus, nostr s corporis induit arts,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="volvit septingents rota temporis anns. "/>
                <para PARENT="Latin"/>
                <ITEXT CH="Et decis septem, sed et nus pne"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" perctus"/>
                <para PARENT="Latin"/>
                <ITEXT CH="nsuper annus erat, cum ire monarchia rgn"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Francrum Carol dvnitus"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" est data Magn."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Nam Carolmannus frter dcesserat"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=" eius"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Presentis iam suprms prope partibus ann. "/>
                <para PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="136.001" YPOS="1123.56687898089" OwnPage="1" ItemID="568058805" PTYPE="4" WIDTH="522" HEIGHT="220.28525477707" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 220.285 L0 220.285 L0 0 Z" copath="M0 0 L522 0 L522 220.285 L0 220.285 L0 0 Z" gXpos="136.001" gYpos="1123.56687898089" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="The Savior of the world, after having deigned to be born of the Virgin, dressed himself in the bodily nature of our flesh; the wheel of time turned 700 years."/>
                <ITEXT CH=" This refers to Jesus Christ being born of the Virgin Mary. Digntus is an active participle because it comes from the deponent verb dgnor. This first phrase acts as a nominative absolute, a construction found in Medieval Latin that is similar to an ablative absolute. The wheel of time is a poetic construction used to convey the passing of time since the Incarnation; see next foonote."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="And 17, and one year almost carried out."/>
                <ITEXT CH=" The date thus given is late AD 717."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" FEATURES="inherit" CH="Furthermore, it was the year when, by divine right, the monarchy of the kingdom of the Franks was given to Charles the Great."/>
                <ITEXT CH=" An English-like order might be "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="cum, dvnitus ire, monarchia rgn Francrum data est Carol Magn"/>
                <ITEXT CH=". "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="dvnitus"/>
                <ITEXT CH=" is an adverb, so it would be literally "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="by right divinely"/>
                <ITEXT CH=". I chose to capitalize "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Magn"/>
                <ITEXT CH=" since it is a common epithet for Charlemagne."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="For Carloman his brother had died now, near the late parts of the present year."/>
                <ITEXT CH=" Carloman died on December 4, 771."/>
                <para PARENT="footnotes"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="766.001" YPOS="888.005" OwnPage="2" ItemID="568300517" PTYPE="4" WIDTH="522" HEIGHT="720" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 720 L0 720 L0 0 Z" copath="M0 0 L522 0 L522 720 L0 720 L0 0 Z" gXpos="766.001" gYpos="888.005" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="ann: year; singular masculine genitive from annus, ann"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="anns: years; plural masculine accusative from annus, ann"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="annus: year; singular masculine nominative from annus, ann"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="arts: bodily nature (in context), sinews, limbs, strengths; plural masculine accusative from artus, arts"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="Carol: Charlemagne, AD 748-814, King of the Franks and Emperor of the Western Roman Empire, member of the Carolingian Dynasty (dative case)"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="Carolmannus: Carloman, son of Pepin the Short and co-ruler of the Frankish kingdom with Charlemagne"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="corporis: body, flesh; singular neuter genitive from corpus, corporis"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="cum: with"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="data: data eset = was given; singular feminine nominative perfect passive participle from d, dare, ded, datus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="d: of, from, away from, down from"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="dcesserat: he had died; 3rd person singular pluperfect active from dcd, dcdere, dcess, dcessus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="decis: ten"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="dgnatus: he had deemed worthy, deigned; singular masculine nominative active perfect passive participle from dgnor, dgnr, dgntus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="divinitus: divinely, of heaven (adverb)"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="eius: his/her/its"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="erat: he was; 3rd person singular imperfect active from sum, esse, fu, futrus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="est: he is; 3rd person singular present active from sum, esse, fu, futrus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="et: and"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="volvit: it rolled out, unrolled; 3rd person singular present active from volv, volvere, volv, voltus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="Francrum: of the Franks"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="frter: brother; singular masculine nominative from frter, frtris"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="iam: already, now, soon"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="induit: he dressed; 3rd person singular perfect active from indu, induere, indu, indtus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="nsuper: besides, furthermore"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="ire: by right; singular neuter ablative from is, iris"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="Mgn: great; singular masculine dative from mgnus, mgna -um, major -or -us, maximus -a -um"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="monarchia: monarchy; singular feminine nominative from monarchia, monarchiae"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="mund: world; singular masculine genitive from mundus, mund"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="nam: yes, truly"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="nsc: to be born; present passive infinitive"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="nostr: our; singular neuter genitive of noster"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="partibus: parts; plural feminine ablative from pars, partis"/>
                <para PARENT="Vocab"/>
