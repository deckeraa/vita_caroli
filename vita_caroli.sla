        <PAGEOBJECT XPOS="136.001" YPOS="888.005" OwnPage="1" ItemID="240878975" PTYPE="4" WIDTH="522" HEIGHT="235.561878980892" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 235.562 L0 235.562 L0 0 Z" copath="M0 0 L522 0 L522 235.562 L0 235.562 L0 0 Z" gXpos="136.001" gYpos="888.005" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Salvātor mundī, postquam dē Virgine nāscī"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Dignātus, nostrī sē corporis induit artūs,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Ēvolvit septingentōs rota temporis annōs. "/>
                <para PARENT="Latin"/>
                <ITEXT CH="Et deciēs septem, sed et ūnus pēne"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" perāctus"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Īnsuper annus erat, cum iūre monarchia rēgnī"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Francōrum Carolō dīvīnitus"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" est data Magnō."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Nam Carolōmannus frāter dēcesserat"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=" eius"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Presentis iam suprēmīs prope partibus annī. "/>
                <para PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="136.001" YPOS="1123.56687898089" OwnPage="1" ItemID="243587439" PTYPE="4" WIDTH="522" HEIGHT="220.28525477707" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 220.285 L0 220.285 L0 0 Z" copath="M0 0 L522 0 L522 220.285 L0 220.285 L0 0 Z" gXpos="136.001" gYpos="1123.56687898089" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="The Savior of the world, after having deigned to be born of the Virgin, dressed himself in the bodily nature of our flesh; the wheel of time turned 700 years."/>
                <ITEXT CH=" This refers to Jesus Christ being born of the Virgin Mary. Dignātus is an active participle because it comes from the deponent verb dīgnor. This first phrase acts as a nominative absolute, a construction found in Medieval Latin that is similar to an ablative absolute. The wheel of time is a poetic construction used to convey the passing of time since the Incarnation; see next foonote."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="And 17, and one year almost carried out."/>
                <ITEXT CH=" The date thus given is late AD 717."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" FEATURES="inherit" CH="Furthermore, it was the year when, by divine right, the monarchy of the kingdom of the Franks was given to Charles the Great."/>
                <ITEXT CH=" An English-like order might be "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="cum, dīvīnitus iūre, monarchia rēgnī Francōrum data est Carolō Magnō"/>
                <ITEXT CH=". "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="dīvĪnitus"/>
                <ITEXT CH=" is an adverb, so it would be literally "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="by right divinely"/>
                <ITEXT CH=". I chose to capitalize "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Magnō"/>
                <ITEXT CH=" since it is a common epithet for Charlemagne."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="For Carloman his brother had died now, near the late parts of the present year."/>
                <ITEXT CH=" Carloman died on December 4, 771."/>
                <para PARENT="footnotes"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="766.001" YPOS="888.005" OwnPage="2" ItemID="243283487" PTYPE="4" WIDTH="522" HEIGHT="720" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 720 L0 720 L0 0 Z" copath="M0 0 L522 0 L522 720 L0 720 L0 0 Z" gXpos="766.001" gYpos="888.005" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
