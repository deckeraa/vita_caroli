        <PAGEOBJECT XPOS="136.001" YPOS="11704.044" OwnPage="27" ItemID="1597935828" PTYPE="4" WIDTH="522" HEIGHT="413.510140127388" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 413.51 L0 413.51 L0 0 Z" copath="M0 0 L522 0 L522 413.51 L0 413.51 L0 0 Z" gXpos="136.001" gYpos="11704.044" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Hesternō Dēsīderiūs diadēmate rēgnī"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Flōruit, ēn hodiē"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=" est pauper, captīvus et exul."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Fīlius illīus, cognōmine dictus Adalgīs,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Cum Langōbardīs in eō spēs ampla manēret,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Diffīdēns rēbus patriae, sē contulit inde"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Ad Cōnstantīnum, Grēcōrum scēptra regentem"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=";"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Ā quō patricius"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" praeclārō mūnere factus,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Hoc in honōre suae permānsit ad ultima vītae."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Rēx autem Carolus veniēns, dum"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=" cognitus eius"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Vēlōx adventus nec dum Saxōnibus esset,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Fēcerat, ut triplex exercitus in regiōnēs"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Illōrum missus, multīs afflīgeret ipsōs"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Cēdibus ac praedīs, loca dēnique plūrima vastāns"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Hinc est cum spoliīs victor regressus opīmīs."/>
                <para PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="136.001" YPOS="12117.5541401274" OwnPage="27" ItemID="1598506020" PTYPE="4" WIDTH="522" HEIGHT="126.489859872601" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 126.49 L0 126.49 L0 0 Z" copath="M0 0 L522 0 L522 126.49 L0 126.49 L0 0 Z" gXpos="136.001" gYpos="12117.5541401274" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" FEATURES="inherit" CH="Yesterday Desiderius flourished in the crown of his kingdom, behold today he is a pauper, captive, and exile."/>
                <ITEXT CH=" The ē in hodiē elides, giving a scansion of DDSSDT."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="To Constantine, reigning the scepters of the Greeks."/>
                <ITEXT CH=" Constantine V was the Emporer of the Eastern Roman/Byzantine Empire from 741 to 775 and gave shelter to Adalgis, the son of Desiderius, who fled to Constantinople."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="By whom he (Adalgis) was made (a) patrician with remarkable employment/office."/>
                <para PARENT="footnotes"/>
                <ITEXT FONT="DejaVu Serif Italic" FEATURES="inherit superscript" CH="4"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="When his switft advance was not yet known to the Saxons."/>
                <trail PARENT="footnotes"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="136.001" YPOS="12536.047" OwnPage="29" ItemID="1602179540" PTYPE="4" WIDTH="522" HEIGHT="534.475292993629" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 534.475 L0 534.475 L0 0 Z" copath="M0 0 L522 0 L522 534.475 L0 534.475 L0 0 Z" gXpos="136.001" gYpos="12536.047" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Annō 775."/>
                <para/>
                <ITEXT CH="Rēx hiemis tempus dūcēns"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=" in Cārisiācō,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Nōmine quō quēdam rēgālis vīlla vocātur,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Illūc Francōrum procerēs tōtumque senātum"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Convocat, ac multum tractāns"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" dē rēbus agendīs,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Et variīs"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=", quibus indiguit rēs pūblica, cūrīs,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Inprīmīs hoc cōnsilium perhibētur inīsse,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Ut iūgiter bellum Saxonibus ingererētur"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=","/>
                <para PARENT="Latin"/>
                <ITEXT CH="Quōs expertus"/>
                <ITEXT FEATURES="inherit superscript" CH="5"/>
                <ITEXT CH=" erat fideī vel foederis omnis"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Immemorēs, nunquam sub pāce quiēscere velle."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Hinc statuit, requiēs"/>
                <ITEXT FEATURES="inherit superscript" CH="6"/>
                <ITEXT CH=" illīs ut nūlla darētur,"/>
                <para PARENT="Latin"/>
                <ITEXT CH="Dōnec gentīlī"/>
                <ITEXT FEATURES="inherit superscript" CH="7"/>
